{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "https-proxy": "dist/cli.js"
  },
  "dependencies": {
    "basic-auth": "2.0.1",
    "commander": "8.2.0",
    "http-status-codes": "2.1.4",
    "logdown": "3.3.1",
    "tsscmp": "1.0.6"
  },
  "description": "A simple HTTPS proxy for Node.js with authentication support.",
  "devDependencies": {
    "@ffflorian/eslint-config": "0.8.1",
    "@ffflorian/prettier-config": "0.1.2",
    "@ffflorian/semantic-release-config": "0.5.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "@types/basic-auth": "1.1.3",
    "@types/jasmine": "3.9.0",
    "@types/node": "~16",
    "@types/tsscmp": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-no-unsanitized": "3.1.5",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "1.8.0",
    "eslint-plugin-unused-imports": "1.1.4",
    "husky": "4.3.8",
    "jasmine": "3.9.0",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "publish-flat": "1.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "typescript"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write"
    ],
    "spec/**/*.ts": [
      "eslint --config .eslintrc.spec.json --fix"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/https-proxy",
  "prettier": "@ffflorian/prettier-config",
  "release": {
    "extends": "@ffflorian/semantic-release-config"
  },
  "repository": "https://github.com/ffflorian/https-proxy.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix && yarn lint:ts:spec --fix",
    "flatten": "publish-flat -o flattened",
    "lint": "yarn lint:ts && yarn lint:ts:spec && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:ts:spec": "eslint --ignore-path .gitignore --ext .ts --config .eslintrc.spec.json spec/",
    "postversion": "node ../node_modules/publish-flat/cli-copy.js -i package.json -o ../package.json version",
    "prettier": "prettier \"*.{json,md,yml}\"",
    "release": "semantic-release",
    "start": "ts-node src/cli.ts",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/.bin/jasmine"
  },
  "version": "1.5.0"
}
